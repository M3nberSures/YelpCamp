<script src="/jquery/jquery-3.3.1.slim.min.js"></script>
<script src="/jquery/popper-1.14.3.min.js"></script>
<script src="/jquery/modernizr-2.8.3.min.js"></script>
<script src="/bootstrap-4.1.3/js/bootstrap.min.js"></script>
<!-- some checkup -->
<% if ( errorlogin.length > 0 || notlogin.length > 0) { %>
    <script>
        $(document).ready(function(){
            $('#modal-login').modal('show');
        });
    </script>
<% }; %>
<% if ( errorregister.length > 0 ) { %>
    <script>
        $(document).ready(function(){
            $('#modal-register').modal('toggle');
        });
    </script>
<% }; %>

<% if ( errorpasswordchange.length > 0 ) { %>
    <script>
        $(document).ready(function(){
            $('#modal-password-change').modal('toggle');
        });
    </script>
<% }; %>
</body>
</html>
